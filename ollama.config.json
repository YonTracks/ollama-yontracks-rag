  "globalSettings": {
    "apiBase": "http://localhost:11434",
    "defaultModel": "llama3.1",
    "defaultVision": "llava-llama3",
    "defaultEmbed": "nomic-embed-text",
    "toolsEnabled": false,
    "visionEnabled": true,
    "timeout": 30000,
    "stream": true
  },
  "models": [
    {
      "model": "llava:latest",
      "name": "llava",
      "description": "Description for llava:latest",
      "settings": {
        "temperature": 0.7,
        "num_ctx": 2048,
        "stream": true
      }
    },
    {
      "model": "llama3.1:latest",
      "name": "llama3.1:latest",
      "description": "Description for llama3.1:latest",
      "settings": {
        "temperature": 0.7,
        "num_ctx": 2048,
        "stream": true
      }
    },
    {
      "model": "llama3.2-mario:latest",
      "name": "llama3.2-mario",
      "description": "Description for llama3.2-mario:latest",
      "settings": {
        "temperature": 0.7,
        "num_ctx": 2048,
        "stream": true
      }
    },
    {
      "model": "llama3.1-code:latest",
      "name": "llama3.1-code",
      "description": "Description for llama3.1-code:latest",
      "settings": {
        "temperature": 0.7,
        "num_ctx": 2048,
        "stream": true
      }
    }
  ],
  "defaultTools": [
    {
      "type": "function",
      "functionName": "getFlightTimes",
      "function": {
        "id": "get_flight_times",
        "name": "get_flight_times",
        "description": "Get the flight times between two cities",
        "parameters": {
          "type": "object",
          "properties": {
            "departure": {
              "type": "string",
              "description": "The departure city (airport code)"
            },
            "arrival": {
              "type": "string",
              "description": "The arrival city (airport code)"
            }
          },
          "required": [
            "departure",
            "arrival"
          ],
          "additionalProperties": false
        }
      }
    },
    {
      "type": "function",
      "functionName": "getCurrentWeather",
      "function": {
        "id": "get_current_weather1",
        "name": "get_current_weather",
        "description": "Get the current weather for a city",
        "parameters": {
          "type": "object",
          "properties": {
            "city": {
              "type": "string",
              "description": "The name of the city"
            }
          },
          "required": [
            "city"
          ],
          "additionalProperties": false
        }
      }
    }
  ]
}